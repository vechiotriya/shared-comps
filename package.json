{
  "name": "shared-comps",
  "version": "1.0.0",
  "description": "Shared Tamagui components for React Native and Next.js",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.mjs",
      "require": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "files": [
    "dist",
    "tamagui.config.ts"
  ],
  "scripts": {
    "build": "tamagui-build",
    "watch": "tamagui-build --watch",
    "clean": "rm -rf dist"
  },
  "license": "ISC",
  "packageManager": "pnpm@10.18.0",
  "dependencies": {
    "@tamagui/core": "^1.136.1",
    "@tamagui/web": "^1.136.1",
    "react": "^19.2.0",
    "react-native": "^0.82.1",
    "react-native-web": "^0.21.2",
    "tamagui": "^1.136.1"
  },
  "devDependencies": {
    "@tamagui/build": "^1.0.0",
    "@tamagui/cli": "^1.136.1",
    "@types/react": "^19.2.2",
    "typescript": "^5.0.0"
  }
}
